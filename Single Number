public class Solution {
    // DO NOT MODIFY THE LIST. IT IS READ ONLY
    public int singleNumber(final List<Integer> A) {
        //Using XOR function
        int n=A.size();
        int ans=0;
        for(int i=0;i<n;i++)
        {
            ans^=A.get(i);         
        }
        return ans;
    }
}
